<script src="https://cdn.bootcss.com/lodash.js/4.17.11/lodash.min.js"></script>
<script src="https://cdn.bootcss.com/vue/2.5.17-beta.0/vue.min.js"></script>
<script src="//cdnjs.cloudflare.com/ajax/libs/gsap/1.18.5/TweenMax.min.js"></script>
<script>

    var app = new Vue({
        el: '#app',
        data: {
            person: [],
            title: "",
            week1: false,
            week4: false,
            randomPerson: [],
            time: 20 //跳动20次
        },
        created: function () {
            var week = new Date().getDay();//获取星期几
            switch (week) {
                case 1:
                    this.title = "Happy x Monday";
                    this.person = [
                        '1', '2', '3', '4', '5', '6', '7', '8'
                    ];
                    this.randomPerson = ['?', '?', '?', '?', '?', '?', '?', '?', '?']
                    this.week1 = true;
                    this.week4 = false;
                    break;
                case 4:
                    this.title = "Happy x Thursday";
                    this.person = [
                        '9', '10', '11', '12', '13', '14','15', '16'

                    ];
                    this.randomPerson = ['?', '?', '?', '?', '?', '?', '?', '?'];
                    this.week1 = false;
                    this.week4 = true;
                    console.log(week + "end");
                    break;
                default:
                    this.title = "happy every day";
                    break;
                    ;
            }

        },
        methods: {
            alert: function () {
                let arr = this.person;
                this.randomPerson = _.shuffle(arr);
                var tuoba = [this.randomPerson[0], this.randomPerson[1], this.randomPerson[2]];
                // console.log(tuoba)
                if (!(tuoba.indexOf('2') === -1) || !(tuoba.indexOf('10') === -1)) {//不在拖地的数组里面 如果在 就重新进行一次
                    console.log("skip")
                    this.alert();
                }
            },
            change: function (index) {
                console.log(index);
                this.person.splice(index, 1, '空');//将点击的人变成空 请假状态
            }
        }

    })




</script>
